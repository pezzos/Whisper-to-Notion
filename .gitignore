# Ignore files and directories generated by various IDEs and editors
.idea/
.vscode/
*.sublime-project
*.sublime-workspace

# Ignore compiled binaries and executables
*.exe
*.dll
*.so
*.dylib

# Ignore dependency directories
node_modules/
vendor/

# Ignore log files and temporary files
*.log
*.tmp
*.swp

# Ignore environment-specific files
.env
.env.local
.env.*.local

# Ignore sensitive or personal files
secrets.txt
credentials.json

# Ignore Python virtual environment
venv/

# Ignore Python bytecode cache
__pycache__/
*.pyc
